{
    "name": "nexmo/laravel",
    "description": "Laravel Package for Nexmo's PHP Client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tim Lytle",
            "email": "tim@nexmo.com",
            "role": "Developer",
            "homepage": "http://twitter.com/tjlytle"
        }
